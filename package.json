  "private": true,
  "homepage": "https://api.greenpeace.org.hk/2019/test/",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "bulma": "^0.8.0",
    "bulma-timeline": "^3.0.4",
    "classnames": "^2.2.6",
    "flexboxgrid": "^6.3.1",
    "formik": "^2.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-id-swiper": "^2.4.0",
    "react-scripts": "3.3.0",
    "sanitize.css": "^11.0.0",
    "swiper": "^5.2.1",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "husky": "^4.0.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react-snap": "^1.23.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postbuild": "react-snap"
  },
  "reactSnap": {
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all",
      "ie 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
